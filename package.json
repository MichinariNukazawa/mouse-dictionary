{
  "name": "mouse-dictionary",
  "version": "1.2.9",
  "repository": "https://github.com/wtetsu/mouse-dictionary.git",
  "author": "wtetsu",
  "license": "MIT",
  "scripts": {
    "build": "npm run build-chrome && npm run build-firefox",
    "watch": "webpack -w --config webpack.config.chrome.js",
    "release": "npm run release-chrome && npm run release-firefox",
    "lint": "eslint src/**/*.js",
    "test": "jest",
    "test-watch": "jest --watch",
    "test-cov": "npm test -- --coverage",
    "build-chrome": "webpack --config webpack.config.chrome.js",
    "build-chrome-beta": "webpack --config webpack.config.chrome.beta.js",
    "watch-chrome": "webpack -w --config webpack.config.chrome.js",
    "release-chrome": "NODE_ENV=production npm run build-chrome",
    "release-chrome-beta": "NODE_ENV=production npm run build-chrome-beta",
    "build-firefox": "webpack --config webpack.config.firefox.js",
    "watch-firefox": "webpack -w --config webpack.config.firefox.js",
    "release-firefox": "NODE_ENV=production npm run build-firefox"
  },
  "dependencies": {
    "@babel/polyfill": "^7.4.4",
    "deinja": "0.0.1",
    "hogan.js": "^3.0.2",
    "immer": "^3.1.3",
    "lodash": "4.17.15",
    "react": "^16.8.6",
    "react-color": "^2.17.3",
    "react-contenteditable": "^3.3.1",
    "react-dom": "^16.8.6",
    "sprintf-js": "^1.1.2",
    "sweetalert": "^2.1.2",
    "uniqlist": "^1.0.4"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.2",
    "babel-jest": "^24.8.0",
    "babel-loader": "^8.0.6",
    "copy-webpack-plugin": "^5.0.4",
    "eslint": "^6.1.0",
    "eslint-plugin-react": "^7.14.3",
    "jest": "^24.8.0",
    "milligram": "^1.3.0",
    "webpack": "^4.38.0",
    "webpack-cli": "^3.3.6"
  }
}
